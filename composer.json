{
    "name": "ahc/strapi-client-bundle",
    "description": "Client bundle for strapi headless cms",
    "type": "symfony-bundle",
    "keywords": ["strapi", "cms", "client", "headless cms", "symfony", "content management"],
    "require": {
        "php": "^7.4",
        "psr/simple-cache": "^1.0",
        "symfony/http-client": "^4||^5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmet Celikezer",
            "email": "ahmet@celikezer.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ahc\\StrapiClientBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.9"
    }
}
